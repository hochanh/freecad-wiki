  <page>
    <title>MediaWiki:Common.css</title>
    <ns>8</ns>
    <id>622</id>
    <revision>
      <id>108071</id>
      <parentid>74174</parentid>
      <timestamp>2014-09-01T19:35:19Z</timestamp>
      <contributor>
        <username>Yorik</username>
        <id>2997</id>
      </contributor>
      <sha1>tfxoklv435fe3oyr1bcpqh82n4ktuww</sha1>
      <text xml:space="preserve" bytes="1577">@import url(&quot;/freecad.css&quot;);
	 	
body.skin-vector h1 {
    letter-spacing: 1px;
    margin: 20px 0;
}

@media screen {
    body.skin-vector #content {
        width: auto;	
        max-width: 1024px;
        margin-left: 14em !important;
        border-radius: 3px;
    }
    body.skin-vector #mw-head-base, #footer {
        margin-left: 14em !important;
        background-image: none !important;
        width: 960px;
    }
}

body.skin-vector #mw-head {
    right: auto !important;
    max-width: 1240px;
}

body.skin-vector #left-navigation {
    left: 14em;
}

body.skin-vector div.vectorTabs, body.skin-vector div.vectorTabs li.selected, body.skin-vector div.vectorTabs ul li, 
body.skin-vector div.vectorTabs span, body.skin-vector div.vectorTabs span, body.skin-vector div.vectorMenu h5 a,
body.skin-vector div.vectorTabs ul {
    background-image: none;
    background-color: transparent;
}
    

body.skin-vector #mw-page-base {
    background: transparent;
    background-image: none;
}

body.skin-vector #mw-panel {
    width: 13em !important;
}

body.skin-vector #mw-panel a, #mw-head a, #p-personal a {
    color: #ddd !important;
}

body.skin-vector #mw-head li.selected a, #mw-head li.selected a:visited {
    color: #bbb !important;
}

.ext-translate-msggroup-selector-menu {
    color: #000 !important;
}

.note {
    background: #eee;
    padding: 5px;
    border-radius: 3px;
}

.editsection {
    font-size: 10px !important;
}

@media print {
    body, #content {
        background: #fff;
   }
   #content p, #content ul {
        font-size: 20px;
   }
}</text>
    </revision>
  </page>
